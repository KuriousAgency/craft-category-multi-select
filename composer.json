{
    "name": "kuriousagency/category-multi-select",
    "description": "Craft CMS category multi select field type ",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "category multi select"
    ],
    "support": {
        "docs": "https://github.com/KuriousAgency/category-multi-select/blob/master/README.md",
        "issues": "https://github.com/KuriousAgency/category-multi-select/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kurious Agency",
            "homepage": "https://kurious.agency"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "kuriousagency\\categorymultiselect\\": "src/"
        }
    },
    "extra": {
        "name": "Category Multi Select",
        "handle": "category-multi-select",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/KuriousAgency/category-multi-select/master/CHANGELOG.md",
        "components": {
            "categoryMultiSelectService": "kuriousagency\\categorymultiselect\\services\\CategoryMultiSelectService"
        },
        "class": "kuriousagency\\categorymultiselect\\CategoryMultiSelect"
    }
}
